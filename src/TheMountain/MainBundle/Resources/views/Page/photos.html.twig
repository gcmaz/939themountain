{% extends 'TheMountainMainBundle::layout.html.twig' %}

{% block title %}
    Photos - 93.9 The Mountain - Flagstaff - Prescott - Northern Arizona
{% endblock %}

{% block description %}
    <meta name="description" content="Photos - 93.9 The Mountain - Flagstaff - Prescott - Northern Arizona"/>
    <meta property="og:site_name" content="http://939themountain.gcmaz.com" />
    <meta property="fb:admins" content="100003228578475" />
    <meta property="fb:app_id" content="159639224186993" />
    <meta property="og:type" content="music.radio_station" />   
    <meta property="og:image" content="http://939themountain.gcmaz.com{{ asset('/images/logo-939themountain.png') }}"/>
    <meta property="og:url" content="http://939themountain.gcmaz.com{{ path('TheMountainMainBundle_photos') }}"/>
    <meta property="og:title" content="Photos - 93.9 The Mountain - Flagstaff - Prescott - Northern Arizona"/> 
    <meta property="og:description" content="Photos - 93.9 The Mountain - Flagstaff - Prescott - Northern Arizona"/>
{% endblock %}

{% block keywords %}
    <meta name="keywords" content="Photos, Flagstaff, Prescott, Northern Arizona"/>
{% endblock %}
{% block styles %}
    {{ parent() }}
    <style type="text/css">
    .photos a{ text-decoration:none; font-weight:700; color:#fff; font-size:1em;line-height:2; }
    .photos-thumb{ padding:10px; }
    </style>
{% endblock %}
 
{% block js_top %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('/scripts/galleria/galleria-1.2.9.js') }}"></script>
    <script type="text/javascript" src="{{ asset('/scripts/galleria/themes/classic/galleria.classic.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('/scripts/galleria/plugins/facebook/galleria.facebook.js') }}"></script>
{% endblock %}
    
{% block content %}
    
    <div class="photos">
        <img src="{{asset('images/photos.png') }}" width="600" height="100" alt="Mountain Photos" class="centered"/>
        <!doctype html>
        <html>
        <body>
        {{ display_block|raw  }}
        </body>
        </html>
    </div>
    <br class="clear"/>
    <div class="fb-like" data-href="http://939themountain.gcmaz.com{{ path('TheMountainMainBundle_photos') }}" data-send="true" data-width="450" data-show-faces="true" data-font="arial"></div>
    <br class="clear"/><br/>
{% endblock %}